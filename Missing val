package training;
import java.util.*;
public class missing_vas 
{
	public static void main(String[] args)
	{
		Scanner in = new Scanner(System.in);
		int n= in.nextInt();
		int arr[]=new int[n];
		int m=n*(n+1)/2;
		for(int i=0;i<arr.length-1;i++)
		{
			arr[i]=in.nextInt();
		}
		for(int i=0;i<arr.length;i++)
		{
			m=m-arr[i];
		}
		System.out.println(m);
	}

}
