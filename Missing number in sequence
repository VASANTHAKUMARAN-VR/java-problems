package training;
import java.util.*;
public class missinginsequence
{
	public static void main(String[] args)
	{
		Scanner in = new Scanner(System.in);
		int n=in.nextInt();
		int arr[]=new int[n];
		int m=n*(n+1)/2;
		int sum = 0;
		for(int i=0;i<n-1;i++)
		{
			arr[i]=in.nextInt();
		}
		for(int i=0;i<arr.length+1;i++)
		{
			m=m-i;
		}
		System.out.println(m);
	}

}
